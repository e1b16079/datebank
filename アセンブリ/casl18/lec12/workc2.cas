PRG1202	START
	LAD	GR1,TABLE
	LAD	GR2,8
	CALL	TABLSUM
	ST	GR0,FULL
	LAD	GR1,4,GR1
	LAD	GR2,4
	CALL	TABLSUM
	ST	GR0,HALF
	RET
TABLSUM	PUSH	0,GR1
	PUSH	0,GR2
	LAD	GR0,0
LOOP	CPA	GR2,=0
	JZE	OWA
	ADDA	GR0,0,GR1
	ADDA	GR1,=1
	ADDA	GR2,=-1
	JUMP	LOOP
OWA	POP	GR2
	POP	GR1
	RET
TABLE	DC	12,-28,39,-44,50,-66,73,-85
FULL	DS	1
HALF	DS	1
	END