#include<stdio.h>

  int sumAtoB(int a,int b){
    int i, sum=0;

    for(i=a;i<=b;i++){
     sum =sum + i;
    }

    return sum;
  }

int main(void){
   int c, d, sum=0;

   printf("開始値を入力して下さい:");
   scanf("%d", &c);
   printf("終了値を入力して下さい:");
   scanf("%d", &d);

   sum=sumAtoB(c,d);

   printf("%dから%dまでの合計は%dです。\n",c,d,sum);

   return 0;
}
